RewriteEngine On    # Turn on the rewriting engine 
RewriteRule ^api/users/([0-9]*)$ api/users/users.php?userId=$1 [NC,L]
RewriteRule ^api/users/userId/([0-9]*)$ api/users/users.php?userId=$1 [NC,L]
RewriteRule ^api/users/([0-9]*)/plants$ api/plants/plants.php?userId=$1 [NC,L]
RewriteRule ^api/plants/plantId/([0-9]*)$ api/plants/plants.php?plantId=$1 [NC,L]
RewriteRule ^api/plants/userId/([0-9]*)$ api/plants/plants.php?userId=$1 [NC,L]
RewriteRule ^api/events/eventId/([0-9]*)$ api/events/events.php?eventId=$1 [NC,L]
RewriteRule ^api/events/plantId/([0-9]*)$ api/events/events.php?plantId=$1 [NC,L]
RewriteRule ^api/images/plantId/([0-9]*)$ api/images/images.php?plantId=$1 [NC,L]
RewriteRule ^api/login/$ api/login/login.php [NC,L]